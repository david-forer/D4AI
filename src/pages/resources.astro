		<!-- SEO & AI Resources Section -->
		<section class="py-16 bg-white">
			<div class="container mx-auto px-6 max-w-6xl">
				<div class="flex flex-col lg:flex-row items-center gap-12">
					<div class="lg:w-1/2">
						<div class="inline-block bg-green-100 text-green-800 text-sm font-bold py-1 px-3 rounded-full mb-4">
